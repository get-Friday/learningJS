function multiplicaVetor(vetor, inteiro) {
    this.vetorRetorno = []
    for (let i in vetor) {
        vetorRetorno.push(vetor[i] * inteiro)
    }
    return vetorRetorno
}

function vetorMaiorCinco() {
    let maiorQueCinco = []
    for (let i in vetorRetorno) {
        if (vetorRetorno[i] > 5) {
            maiorQueCinco.push(vetorRetorno[i])
        }
    }
    return maiorQueCinco
}
const vetor = [1, 2, 3, 4, 5]
console.log(multiplicaVetor(vetor, 3))
console.log(vetorMaiorCinco())
